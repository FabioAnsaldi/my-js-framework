{
  "name": "my-js-framework",
  "version": "0.1.0",
  "description": "Personal JavaScript framework to develop web applications.",
  "main": "index.js",
  "scripts": {
    "server": "export PORT=4321 && node ./node_modules/.bin/tiny-server ./",
    "timeout": "sleep 1",
    "echo:index": "npm run timeout && node -e 'console.log(\"http://localhost:4321/index.html\")'",
    "start": "npm run echo:index & npm run server & wait",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/FabioAnsaldi/my-js-framework.git"
  },
  "keywords": [
    "JavaScrip",
    "Framework",
    "Web"
  ],
  "author": "Fabio Ansaldi",
  "license": "GPL-3.0-or-later",
  "bugs": {
    "url": "https://github.com/FabioAnsaldi/my-js-framework/issues"
  },
  "homepage": "https://github.com/FabioAnsaldi/my-js-framework#readme",
  "devDependencies": {
    "tiny-server": "^1.1.1"
  }
}
